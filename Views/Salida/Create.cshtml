@model MigracionControl.Models.Salida

<h2>Registrar Nueva Salida</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="FechaSalida"></label>
        <input asp-for="FechaSalida" class="form-control" />
        <span asp-validation-for="FechaSalida" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LugarSalida"></label>
        <input asp-for="LugarSalida" class="form-control" />
        <span asp-validation-for="LugarSalida" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Registrar</button>
</form>
