@model MigracionControl.Models.Entrada

<h2>Editar Entrada</h2>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="FechaEntrada"></label>
        <input asp-for="FechaEntrada" class="form-control" />
        <span asp-validation-for="FechaEntrada" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LugarEntrada"></label>
        <input asp-for="LugarEntrada" class="form-control" />
        <span asp-validation-for="LugarEntrada" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Guardar Cambios</button>
</form>
